echo "bench,bpred,size,frmt,metric,value" > compiled_results/ob.csv
ls results/*ob*.txt | xargs -I % sh -c 'grep -H "bpred_.*" %' | sed -r 's|results/||g' | sed -r 's|\..*:|,|' | sed -r 's|bpred_[^\_]+\.||g' | sed -r 's|\s+#.*||g' | sed -r 's|_ob_|,ob,|g' | sed -r 's|\s+|,|g' | sed -r 's|[,]+|,|g' | sed -r 's|B,|B,0,|g' | sed -r 's|_frmt|,1|g' >> compiled_results/ob.csv
ls results/*ob*.txt | xargs -I % sh -c 'grep -H "sim_.*" %' | sed -r 's|results/||g' | sed -r 's|\..*:|,|' | sed -r 's|bpred_[^\_]+\.||g' | sed -r 's|\s+#.*||g' | sed -r 's|_ob_|,ob,|g' | sed -r 's|\s+|,|g' | sed -r 's|[,]+|,|g' | sed -r 's|B,|B,0,|g' | sed -r 's|_frmt|,1|g' >> compiled_results/ob.csv
cp compiled_results/ob.csv compiled_results/ob.csv_tmp
grep -v "fpppp" compiled_results/ob.csv > compiled_results/ob.csv_tmp
grep -v "m88ksim" compiled_results/ob.csv_tmp > compiled_results/ob.csv
grep -v "vortex" compiled_results/ob.csv > compiled_results/ob.csv_tmp
mv compiled_results/ob.csv_tmp compiled_results/ob.csv
